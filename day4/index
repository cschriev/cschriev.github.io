<html>
<head>
	<title>jQurey</title>
<script type="text/javascript" src="C:\Users\Cathrin\Desktop/jquery-1.11.3.js"></script>
 
<script type="text/javascript">
 
	$(document).ready(function(){
 
		console.log("jQuery is working");	

        $("button").click(function(){
            var animal = $(this).text();
            console.log(animal);

        var flickrURL = "http://api.flickr.com/services/feeds/photos_public.gne?jsoncallback=?";

        $.ajax({
        url: flickrURL,
        dataType: "jsonp",
        data: {
            tags: animal,
            format : 'json',
        },
        success: function(flickrData) {
            console.log(flickrData.items)

            var newArray = []
            var resultsArray = flickrData.items
        for (var i = 0; i < resultsArray.length; i++){
        var track = resultsArray[i].media.m;
        newArray.push(track);
        
        }
          console.log(newArray);

        var htmlList = "";
        for (var i = 0; i < resultsArray.length; i++){
        var picture = resultsArray[i].media.m;
        console.log(picture);
        htmlList += "<img src='" + picture + "'>";
        }
        $("#searchResults").append(htmlList);

    
        $("button").click(function(){
        $("p").empty();
        });

    }
        });
        });

});
 

	</script>
</head>
<body>
<button>dog</button>
<button>cat</button>
<button>bird</button>

<p id="searchResults">
</p>

</body>
</html>